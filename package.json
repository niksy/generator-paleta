{
  "name": "@niksy/generator-paleta",
  "version": "10.1.1",
  "description": "Yeoman generator for my projects.",
  "license": "MIT",
  "author": "Ivan NikoliÄ‡ <niksy5@gmail.com> (http://ivannikolic.com)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./generators/app/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "generators/app/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "generators/",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint '{generators/app/index,test/index}.js'",
    "prepare": "if [[ -x \"$(command -v husky)\" ]]; then husky; fi",
    "prerelease": "npm run lint",
    "release": "np --no-release-draft",
    "test": "NODE_OPTIONS='--experimental-loader=@istanbuljs/esm-loader-hook --no-warnings' nyc mocha 'test/**/*.js' && nyc check-coverage",
    "test:watch": "nodemon --exec npm test"
  },
  "dependencies": {
    "browserslist": "^4.27.0",
    "is-github-url": "^1.2.2",
    "is-scoped": "^3.0.0",
    "lodash": "^4.17.21",
    "parse-github-url": "^1.0.3",
    "path-exists": "^5.0.0",
    "sort-package-json": "^3.4.0",
    "yeoman-generator": "^7.5.1"
  },
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "^0.2.0",
    "eslint": "^9.39.1",
    "eslint-config-nitpick": "^14.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.0.11",
    "lint-staged": "^16.2.6",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "np": "^10.0.2",
    "nyc": "^15.1.0",
    "prettier": "^3.6.2",
    "write-json-file": "^7.0.0",
    "yeoman-environment": "^5.0.0",
    "yeoman-test": "^11.0.0"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niksy/generator-paleta.git"
  },
  "bugs": {
    "url": "https://github.com/niksy/generator-paleta/issues"
  },
  "homepage": "https://github.com/niksy/generator-paleta#readme"
}
