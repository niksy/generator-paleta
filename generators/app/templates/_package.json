{
	"name": "<%= moduleName %>",
	"version": "<% if ( version ) { %><%= version %><% } else { %>0.0.0<% } %>",
	"description": "<%= moduleDescription %>",
	"type": "module"<% if ( !cli ) { %>,
	"main": "<% if ( sassModule ) { %>_index.scss<% } else if ( cssModule ) { %>index.css<% } else { %><% if ( transpile ) { %>dist/<% } %>index.<% if ( bundleCjs ) { %>cjs<% } else { %>js<% } %><% } %>"<% if ( browserModule && !sassModule ) { %>,
	"module": "<% if ( transpile ) { %>dist/<% } %>index.js"<% } %>,
	"exports": {
		".": {<% if ( typescript ) { %>
			"types": "./dist/index.d.ts",<% } %><% if ( browserModule && styles || sassModule || cssModule ) { %><% if ( sassModule ) { %>
			"sass": "./_index.scss",<% } %>
			"style": "./<% if ( sassModule ) { %>_index.scss<% } else if ( cssModule ) { %>index.css<% } else { %><% if ( transpile ) { %>dist/<% } %>index.css<% } %>",<% } %>
			"import": "./<% if ( transpile ) { %>dist/<% } %>index.js"<% if ( bundleCjs ) { %>,
			"require": "./dist/index.cjs"<% } %>
		},
		"./package.json": "./package.json"
	}<% if ( browserModule && !sassModule ) { %>,
	"sideEffects": false<% } %><% } %>,
	"author": "<%= humanName %> <<%= email %>> (<%= website %>)",
	"files": [<% if ( !transpile ) { %>
		"<% if ( browserModule && styles || sassModule || cssModule ) { %><% if ( sassModule ) { %>_index.scss<% } else if ( cssModule ) { %>index.css<% } else { %>index.css<% } %><% } else { %>index.js<% } %>",<% } %><% if ( transpile ) { %>
		"dist/",<% } else { %>
		"lib/",<% } %><% if ( changelog ) { %>
		"CHANGELOG.md",<% } %>
		"LICENSE.md",
		"README.md"
	],
	"license": "MIT"<% if ( browserModule && styles || sassModule || cssModule ) { %>,
	"style": "<% if ( sassModule ) { %>_index.scss<% } else if ( cssModule ) { %>index.css<% } else { %><% if ( transpile ) { %>dist/<% } %>index.css<% } %>"<% } %><% if ( cli ) { %>,
	"bin": {
		"<%= cliCommandName %>": "cli.js"
	}<% } %><% if ( manualTests || automatedTests || integrationTests ) { %>,
	"directories": {
		"test": "test"
	}<% } %>,
	"scripts": {
		"prepare": "if [[ -x \"$(command -v husky)\" ]]; then husky; fi",
		"release": "np --no-release-draft"<% if ( changelog ) { %>,
		"version": "version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md"<% if ( githubRelease ) { %>,
		"postpublish": "GITHUB_TOKEN=$GITHUB_RELEASE_TOKEN github-release-from-changelog"<% } %><% } %>,<% if ( (automatedTests && browserModule && !sassModule) || manualTests || integrationTests ) { %>
		"lint": "<% if ( automatedTests || integrationTests ) { %>eslint '{index,lib/**/*<% if ( cli ) { %>,cli<% } %>,test/**/*}<% if ( extensionsToProcess.length > 1 ) { %>.{<%- extensionsToProcess.join(',') %>}<% } else { %>.js<% } %>'<% } else { %>eslint '{index,lib/**/*<% if ( cli ) { %>,cli<% } %>}<% if ( extensionsToProcess.length > 1 ) { %>.{<%- extensionsToProcess.join(',') %>}<% } else { %>.js<% } %>'<% } %><% if ( browserModule && styles ) { %> && stylelint 'index.css'<% } %>"<% } else if ( sassModule ) { %>
		"lint": "<% if ( automatedTests ) { %>eslint 'test/**/*<% if ( extensionsToProcess.length > 1 ) { %>.{<%- extensionsToProcess.join(',') %>}<% } else { %>.js<% } %>' && stylelint '{_index,test/**/*}.scss'<% } else { %>stylelint '_index.scss'<% } %>"<% } else { %>
		"lint": "<% if ( automatedTests ) { %>eslint '{index,lib/**/*<% if ( cli ) { %>,cli<% } %>,test/**/*}<% if ( extensionsToProcess.length > 1 ) { %>.{<%- extensionsToProcess.join(',') %>}<% } else { %>.js<% } %>'<% } else { %>eslint '{index,lib/**/*<% if ( cli ) { %>,cli<% } %>}<% if ( extensionsToProcess.length > 1 ) { %>.{<%- extensionsToProcess.join(',') %>}<% } else { %>.js<% } %>'<% } %><% if ( browserModule && styles ) { %> && stylelint 'index.css'<% } %>"<% } %><% if ( typescript ) { %>,
		"lint:types": "tsc"<% } %><% if ( automatedTests && browserModule && !sassModule ) { %>,
		"test:automated": "karma start",
		"test:automated:watch": "npm run test:automated -- --auto-watch --no-single-run"<% } %><% if ( integrationTests ) { %>,
		"test:integration": "wdio",
		"test:integration:watch": "WATCH_MODE=true wdio"<% } %><% if ( manualTests ) { %>,
		"test:manual": "vite --config ./test/manual/vite.config.js"<% } %><% if ( (automatedTests && browserModule && !sassModule) || manualTests || integrationTests ) { %>,
		"test": "<% if ( automatedTests && browserModule && !sassModule ) { %>npm run test:automated<% } %><% if ( integrationTests ) { %> && npm run test:integration<% } %>"<% } else if ( sassModule ) { %>,
		"test": "<% if ( automatedTests ) { %>mocha 'test/**/*.<%= extension || 'js' %>'<% } %>"<% if ( automatedTests ) { %>,
		"test:watch": "npm test -- --watch"<% } %><% } else { %>,
		"test": "<% if ( automatedTests ) { %><% if ( codeCoverage ) { %>nyc --check-coverage mocha<% } else { %>mocha<% } %> 'test/**/*.<%= extension || 'js' %>'<% } else { %><% } %>"<% if ( automatedTests ) { %>,
		"test:watch": "<% if ( codeCoverage ) { %>nodemon<% if ( extensionsToProcess.length > 1 ) { %> --ext <%- extensionsToProcess.join(',') %>,json<% } %> --exec npm test<% } else { %>npm test -- --watch<% } %>"<% } %><% if ( codeCoverageService ) { %>,
		"posttest:ci": "cat ./coverage/lcov.info | coveralls"<% } %><% } %><% if ( (transpile && !browserModule) || (browserModule && !sassModule) ) { %>,
		"build": "rolldown --config rolldown.config.js",
		"prepublishOnly": "npm run build"<% if ( bundleCjs ) { %>,
		"module-check": "node -e 'require(\"<%= moduleName %>\");' && node --input-type=module -e 'import \"<%= moduleName %>\";'"<% } %><% } %>,
		"prerelease": "npm run lint<% if ( typescript ) { %> && npm run lint:types<% } %><% if ( (transpile && !browserModule) || (browserModule && !sassModule) ) { %> && npm run build<% if ( bundleCjs ) { %> && npm run module-check<% } %><% } %>"
	},
	"devDependencies": {
		"husky": "^9.0.11",
		"lint-staged": "^15.5.2",
		"np": "^10.0.2"<% if ( prettier ) { %>,
		"prettier": "^3.6.2"<% } %>,
		"eslint": "^9.39.1",
		"eslint-config-nitpick": "^14.1.0"<% if ( prettier ) { %>,
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4"<% } %><% if ( changelog ) { %>,
		"version-changelog": "^3.1.1",
		"changelog-verify": "^1.1.2"<% if ( githubRelease ) { %>,
		"github-release-from-changelog": "^2.1.1"<% } %><% } %><% if ( browserModule && styles ) { %>,
		"stylelint": "^16.25.0",
		"stylelint-config-nitpick": "^11.1.0"<% if ( prettier ) { %>,
		"stylelint-prettier": "^5.0.3"<% } %><% } %><% if ( automatedTests && browserModule && !sassModule ) { %>,
		"@types/karma": "^6.3.9",
		"karma": "^6.4.3",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-rolldown-preprocessor": "^1.0.0"<% if ( cloudBrowsers ) { %>,
		"@types/karma-browserstack-launcher": "^1.6.5",
		"karma-browserstack-launcher": "^1.6.0"<% } %>,
		"@types/karma-chrome-launcher": "^3.1.4",
		"karma-chrome-launcher": "^3.1.0"<% if ( browserTestType === 'headless' ) { %>,
		"puppeteer": "^22.6.1"<% } %><% if ( usesHtmlFixtures ) { %>,
		"@types/karma-html2js-preprocessor": "^1.1.0",
		"karma-html2js-preprocessor": "^1.1.0",
		"@types/karma-fixture": "^0.2.8",
		"karma-fixture": "^0.2.6"<% } %>,
		"@types/karma-mocha": "^1.3.4",
		"karma-mocha": "^2.0.1",
		"@types/karma-mocha-reporter": "^2.2.4",
		"karma-mocha-reporter": "^2.2.5"<% } %><% if ( automatedTests || integrationTests ) { %>,
		"mocha": "^10.3.0"<% } %><% if ( automatedTests && codeCoverage && !browserModule ) { %>,
		"nodemon": "^3.1.0",
		"@istanbuljs/esm-loader-hook": "^0.3.0",
		"nyc": "^15.1.0"<% } %><% if ( automatedTests && codeCoverage && browserModule && !sassModule ) { %>,
		"rollup-plugin-istanbul": "^5.0.0",
		"@types/karma-coverage": "^2.0.3",
		"karma-coverage": "^2.0.3"<% } %><% if ( codeCoverageService ) { %>,
		"coveralls": "^3.1.1"<% } %><% if ( manualTests || integrationTests ) { %>,
		"vite": "npm:rolldown-vite@^7.2.1",
		"suitcss-components-test": "^1.0.0"<% } %><% if ( automatedTests && browserModule && !sassModule ) { %>,
		"unenv": "^2.0.0-rc.24"<% } %><% if ( sassModule ) { %>,
		"sass-true": "^8.0.0"<% } %><% if ( integrationTests ) { %><% if ( cloudBrowsers ) { %>,
		"@wdio/browserstack-service": "^9.20.1"<% } %>,
		"wdio-docker-service": "^3.2.1",
		"@wdio/mocha-framework": "^9.20.1",
		"@wdio/spec-reporter": "^9.20.0",
		"@wdio/cli": "^9.20.1",
		"@wdio/globals": "^9.17.0",
		"@wdio/local-runner": "^9.20.1",
		"webdriverio": "^9.20.1"<% } %><% if ( transpile && automatedTests && !browserModule ) { %>,
		"@swc-node/register": "^1.11.1"<% } %><% if ( typescript ) { %>,
		"typescript": "^5.9.3"<% } %>,
		"@types/node": "^<%= nodeEngineVersion.major %>"<% if ( automatedTests || integrationTests ) { %>,
		"@types/mocha": "^10"<% } %><% if ( (transpile && !browserModule) || (browserModule && !sassModule) ) { %>,
		"rolldown": "^1.0.0-beta.47"<% if ( typescript ) { %>,
		"rolldown-plugin-dts": "^0.17.3"<% } %><% } %>
	}<% if ( isScopedPackage ) { %>,
	"publishConfig": {
		"access": "public"
	}<% } %>,
	"engines": {
		"node": "<%- nodeEngineVersion.raw %>"
	}<% if ( keywords.length > 0 ) { %>,
	"keywords": [<% for ( var i = 0, keywordsLength = keywords.length; i < keywordsLength; i++ ) { %>
		"<%= keywords[i] %>"<% if ( i !== keywordsLength-1 ) { %>,<% } %>
	<% } %>]<% } %>,
	"repository": {
		"type": "git",
		"url": "git+https://<%= gitRepo.host %>/<%= gitRepo.repository %>.git"
	},
	"bugs": {
		"url": "https://<%= gitRepo.host %>/<%= gitRepo.repository %>/issues"
	},
	"homepage": "https://<%= gitRepo.host %>/<%= gitRepo.repository %>#readme"
}
