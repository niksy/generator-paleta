{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",
  "main": "index.js",
  "author": "Ivan NikoliÄ‡ <niksy5@gmail.com> (http://ivannikolic.com/)",
  "license": "MIT",<% if ( manualTests || automatedTests ) { %>
  "directories": {
    "test": "test"
  },<% } %>
  "scripts": {
    "test": "<% if ( automatedTests ) { %>eslint {index,test/*}.js && mocha test/index.js<% } else { %>eslint index.js<% } %>"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-niksy": "^1.0.6"<% if ( automatedTests ) { %>,
    "mocha": "^2.4.5"<% } %>
  },
  "engines": {
    "node": ">=0.10.0"
  },<% if ( keywords.length > 0 ) { %>
  "keywords": [<% for ( var i = 0, keywordsLength = keywords.length; i < keywordsLength; i++ ) { %>
  	"<%= keywords[i] %>"<% if ( i !== keywordsLength-1 ) { %>,<% } %>
  <% } %>],<% } %>
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubRepo %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= githubRepo %>/issues"
  },
  "homepage": "https://github.com/<%= githubRepo %>#readme"
}
